                        .phone-icon-slide{
                            width: 300px;
                            transition: all .6s ease-in-out;
                            p{
                              opacity: 1; transition: all .6s ease-in-out;
                              width: 100%;
                            }
                        } 
                    }

                }
                a.contact-icon {
                    
                    display: flex;
                    align-items: center;
                    justify-content: flex-end;
                    position: relative;
                      
                    .icon-svg{
                          
                        display: flex;
                        justify-content: center;
                        align-items: center;
                        background: #FFFFFF;
                        box-shadow: 0 5px 17px 0 rgba(0,0,0,0.06);
                        border-radius: 100%;
                        height: 72px;
                        width: 72px;
                        z-index: 2;
                        position: relative;
                        
                    }
                  
                    .contact-icon-slide{
                        height: 72px;
                        width: 72px;
                        background-color: #F2F2F2;
                        border-radius: 100px;
                        position: absolute;
                        right: 0;
                        top: 0;
                        transition: all .6s ease-in-out;
                        z-index: 1;
                        display: flex;
                        align-items: flex-start;
                        justify-content: center;
                        padding: 20px 30px;
                        p{opacity: 0; transition: all .6s ease-in-out;
                          width: 0;
                          text-align: left;
                          overflow: hidden;
                          line-height: normal;
                      }
                    }
                    
                    &:hover {
                        
                        .contact-icon-slide{
                            width: 300px;
                            transition: all .6s ease-in-out;
                            p{opacity: 1; transition: all .6s ease-in-out;
                            width: 100%;}
                        } 
                    }
                    
                }
                transition: all .6s ease-in-out;
                &:hover {
                    margin-left: 220px;
                    transition: all .6s ease-in-out;
                }
                
            }
        }
    }
  }
}

