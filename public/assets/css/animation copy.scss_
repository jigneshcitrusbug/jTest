          }
          .icon-behance {
            -webkit-mask: url(../images/icons/Behance.svg) no-repeat 50% 50%;
            mask: url(../images/icons/Behance.svg) no-repeat 50% 50%;
            -webkit-mask-size: contain;
            height: 16px;
            width: 24px;
          }
        }
        &:after {
          display: none;
        }
      }
    }
  }
  .send-message {
    text-align: right;
    justify-content: flex-end;
    padding: 0;
    margin: 0px 0 0px 0;
    position: relative;
    display: flex;
    ul {
      display: flex;
      margin-bottom: 0;
      li {
        display: inline-block;

        position: relative;
        margin-right: 10px;
        &:last-child {
          margin-right: 0;
        }
        a.contact-icon {
          display: flex;
          align-items: center;
          justify-content: flex-start;
          position: relative;

          .icon-svg {
            display: flex;
            justify-content: center;
            align-items: center;
            background: #ffffff;
            box-shadow: 0 5px 17px 0 rgba(0, 0, 0, 0.06);
            border-radius: 100%;
            height: 48px;
            width: 48px;
            z-index: 2;
            position: relative;
          }

          .contact-icon-slide {
            height: 48px;
            width: 48px;
            background-color: #f2f2f2;
            border-radius: 100px;
            position: absolute;
            left: 0;
            top: 0;
            transition: all 0.6s ease-in-out;
            z-index: 1;
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 8px 20px;
            p {
              opacity: 0;
              transition: all 0.6s ease-in-out;
              width: 0;
              text-align: left;
              overflow: hidden;
              line-height: normal;
              font-size: 1rem;
            }
          }

          &:hover {
            .contact-icon-slide {
              padding-left: 55px;
              padding-right: 15px;
              width: 300px;
              transition: all 0.6s ease-in-out;
              p {
                opacity: 1;
                transition: all 0.6s ease-in-out;
                width: 100%;
              }
            }
          }
        }
        transition: all 0.6s ease-in-out;
        &:hover {
          margin-right: 260px;
          transition: all 0.6s ease-in-out;
        }
      }
    }
  }
}

