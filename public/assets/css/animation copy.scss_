        color: #ffffff;
      }
      .inner-page .banner-workdetail-page .banner-div .content-div .heading-workdetail .view-allprojects a {
        color: #ffffff !important;
      }

      .menubar-header,
      .new-project {
        // background-color: rgba(0, 0, 0, 0.1);
        .header-div {
          .nav-center-div {
            .nav-m-bar {
              .menu-icon {
                .bar {
                  .fill {
                    background-color: white;
                  }
                }
              }
            }
          }
        }
        .new-project-link-middle {
          .transform-link {
            color: rgba(255, 255, 255, 0.8);
          }
        }
      }
      .arrow-right-circle {
        background-color: rgba(255, 255, 255, 0.8);
      }
    }
    .line-1 {
      opacity: 0;
    }
  }
  &[theme="dark"] {
    .menubar-header,
    .new-project {
      // background-color: rgba(0, 0, 0, 0.1);
      .header-div {
        .nav-center-div {
          .nav-m-bar {
            .menu-icon {
              .bar {
                .fill {
                  background-color: white;
                }
              }
            }
          }
        }
        .header-work-text a {
          color: #fff;
        }
      }
      .new-project-link-middle {
