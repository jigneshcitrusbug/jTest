.button-common-div {
  padding: 0;
  margin-left: 0px;
  position: relative;
  width: auto;
  // max-width: 1100px;
  display: inline-block;
  .btn-common {
    padding: 10px 25px 10px 25px;
    margin: 0;
    font-size: 24px;
    color: #727272;
    height: 60px;
    font-weight: 300;
    display: flex;
    justify-content: space-between;
    line-height: 36px;
    transition: all 0.8s;
    position: relative;
    background-color: transparent;
    overflow: hidden;

    &.btn-black {
      &:hover {
        .btn-background {
          background-color: #fff;
        }
        .transform-text {
          color: #fff;
        }
      }
      .btn-background {
        background-color: #000;
      }
      .arrow-right-circle {
        background-color: #000;
      }
    }
    &.btn-white {
      .transform-text {
        color: #fff;
      }
      .btn-background {
        background-color: #000;
      }
      .arrow-right-circle {
        background-color: #fff;
      }

      &:hover {
        .btn-background {
          background-color: #000;
        }
        .transform-text {
          color: #000;
        }
      }
    }

    &:hover {
      .btn-background {
        clip-path: circle(12% at 90% 50%);
      }
      .transform-text {
        transform: translate(-30px, 0px);
      }
      .arrow-right-circle {
        opacity: 1;
        right: 7px;
      }
    }
    .btn-background {
      position: absolute;
      height: 100%;
      width: 100%;
      left: 0;
      top: 0;
      z-index: 0;
      border-radius: 40px;
      transition: all 0.8s;
      clip-path: circle(100% at 50% 50%);
    }
    .transform-text {
      position: relative;
      z-index: 1;
      transition: all 0.8s;
    }
    .arrow-right-circle {
      margin: 0 0 0 25px;
      transition: 0.8s;
      opacity: 0;
      position: absolute;
      right: -100px;
      top: 0;
      z-index: 1;
      transition: all 0.8s;
      top: 9px;
    }
    &:hover,
    &:focus {
      outline: none;
      box-shadow: none;
    }
  }
}

