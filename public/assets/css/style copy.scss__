@charset "utf-8";
// @import url(bootstrap.min.css);
@import url(custom.css);
// @import url(animate.min.css);
// @import url(owl.carousel.min.css);
// @import url(owl.theme.default.css);
@import url(../fonts/feather/feather.min.css);
@import url(https://fonts.googleapis.com/css?family=Poppins:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap);

/* colors */

$transparent-color: transparent;

$white-color: rgb(255, 255, 255);
$light-white-color: rgba(255, 255, 255, 0.6);
$light-white-color2: rgba(255, 255, 255, 0.5);

$black-color:rgba(0, 0, 0, 1);
$black-color-40:rgba(0, 0, 0, 0.4);

$primary-color1: #ed5c37;
$primary-color2: #181818;

$sub-color1: #EFF5F7;
$sub-color1-50: rgba(239, 245, 247, 0.5);

$sub-color2: #727272;
$sub-color3: #99A7A8;
$sub-color4: #FAFAFA;

$color1:#FF2D3D;
$color2:#239FF1;
$color3:#F26820;
$color4:#017363;


/* End of colors */

/* default css */

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video { margin: 0; padding: 0; border: 0; font-size: 100%; vertical-align: baseline;  }
*, *:before, *:after { -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; -webkit-overflow-scrolling: touch; }
html { text-rendering: optimizeLegibility; -webkit-text-size-adjust: 100%; -moz-text-size-adjust: 100%; -ms-text-size-adjust: 100%; text-size-adjust: 100%; scroll-behavior: smooth; }
body { font-size: 14px; -webkit-font-smoothing: antialiased; -webkit-text-size-adjust: 100%; background-color: $white-color; font-family: 'Poppins', sans-serif !important; scroll-behavior: smooth; line-height: 1.5;  }

.wrapper {  background-color: $white-color; display: block; height: 100%;   
 }
.container { max-width: 1200px; }
.container-lg{ max-width: 1300px; }
.container-fluid{ width: 100%; padding: 0 60px 0 78px; }

.max3-lines{ display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 3; overflow: hidden; }

.p0{ padding-left: 0px; padding-right: 0px; }
.span-block{ display: block; }
p { padding: 0; margin: 0; font-size: 24px; font-weight: 300; color: $sub-color2; }

.mt-10{ margin-top: 10px; }
.mt-20{ margin-top: 20px; }
.mt-30{ margin-top: 30px; }
.mt-40{ margin-top: 40px; }
.mt-50{ margin-top: 50px !important; }
.mt-60{ margin-top: 60px !important; }
.mt-65{ margin-top: 65px !important; }
.mt-80{ margin-top: 80px !important; }

.mb-0{ margin-bottom: 0px; }
.mb-10{ margin-bottom: 10px; }
.mb-20{ margin-bottom: 20px; }
.mb-30{ margin-bottom: 30px; }
.mb-40{ margin-bottom: 40px; }
.mb-45{ margin-bottom: 45px; }
.mb-50{ margin-bottom: 50px; }

ul{ list-style: none; }

button{ transition: 0.5s; }

.primary-color2{ color:$primary-color2 !important; }

/* End of default css */

.w100{ width: 100% !important; max-width: 100% !important; }

.heading-div{
    padding: 0; margin: 0 0 10px 0; 
    h2{ padding: 0; margin: 0 0 40px 0; font-size: 3.625rem; font-weight: 600; color: $white-color; line-height: 72px; }
    .link1{ font-weight: 600; position: relative; color: $white-color; padding: 0 5px 0 0; text-decoration: none; text-decoration-color: $primary-color1; position: relative; 
        &::before{ content: ""; position: absolute; bottom: 8px; left: 3px; width: 45px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
        &::after{ content: ""; position: absolute; bottom: 8px; right: 5px; width: 170px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
    }
    .link2{ font-weight: 600; position: relative; color: $black-color; padding: 0 5px 0 0; text-decoration: none; text-decoration-color: $primary-color1;
        &::before{ content: ""; position: absolute; bottom: 8px; left: 3px; width: 98px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
        &::after{ content: ""; position: absolute; bottom: 8px; right: 50px; width: 160px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
    }
    .link3{ font-weight: 600; position: relative; color: $black-color; padding: 0 5px 0 0; text-decoration: none; text-decoration-color: $primary-color1; display: inline-block;
        &::before{ content: ""; position: absolute; bottom: 8px; left: 3px; width: 98px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
        &::after{ content: ""; position: absolute; bottom: 8px; right: 15px; width:30px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
    }
    .link4{ font-weight: 600; position: relative; color: $black-color; padding: 0 5px 0 0; text-decoration: none; text-decoration-color: $primary-color1; display: inline-block;
        &::before{ content: ""; position: absolute; bottom: 8px; left: 3px; width: 98px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
    }
    .link5{ font-weight: 600; position: relative; color: $white-color; padding: 0 5px 0 0; text-decoration: none; text-decoration-color: $primary-color1; position: relative; 
        &::before{ content: ""; position: absolute; bottom: 8px; left: 3px; width: 210px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
        &::after{ content: ""; position: absolute; bottom: 8px; right: 45px; width: 250px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
        span.line-5span{ position: relative;
            &::after{ content: ""; position: absolute; bottom: 8px; left: 0px; width: 25px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
        }
    }
    .link6{ font-weight: 600; position: relative; color: $black-color; padding: 0 5px 0 0; text-decoration: none; text-decoration-color: $primary-color1; display: inline-block;
        &::before{ content: ""; position: absolute; bottom: 0px; left: 0px; width: 80px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
        &::after{ content: ""; position: absolute; bottom: 0px; right: 0px; width:90px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; }
    }
    .link7{ 
        position: relative;
        &::before{ 
            content: ""; position: absolute; bottom: 8px; left: 40px; width: 125px; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; 
        }
    }
    .link-hr{ 
        position: relative;
        &::before{ 
            content: ""; position: absolute; bottom: 8px; left: 0; width: 100%; height: 5px; display: inline-block; border-radius: 100px; background-color:  $primary-color1; 
        }
        &.link-hr-a3{
            &::before{ width: 40px; left: 5px; }
        }
    }
    
    &.heading-center-div{
        text-align: center;
        justify-content: center;
        margin: 0 auto 10px auto;
    }
}

.button-common-div{
    padding: 0; margin: 40px auto 0 auto; position: relative; width: 100%; max-width: 1100px; display: flex;
    .btn-common{ padding: 10px 10px 10px 25px; margin: 0; font-size: 24px; color: $sub-color2; height: 60px; font-weight: 300;  background-color: $black-color; border-radius: 40px; display: flex; justify-content: space-between; line-height: 45px;  transition: 0.8s;
        .arrow-right-circle{ background-color: $white-color; margin: 0 0 0 25px; transition: 0.8s; }
        &:hover, &:focus{
            outline: none; box-shadow: none;
        }

        &.btn-white{
            background-color: $white-color; color: $black-color; align-items: center;
            .arrow-right-circle{ background-color: $black-color; width: 40px; }
        }

        &.btn-red{
            background-color: $primary-color1; color: $white-color; align-items: center;
            .arrow-right-circle{ background-color: $white-color; width: 40px; }
        }

        &.btn-black-common{
            align-items: center; color:$white-color;

        }
    }
    &.button-common-div2{
        max-width: 100%; 
    }
}

.btn.btn-messages{
    padding: 10px 25px;
    font-size: 16px;
    color: #FFFFFF;
    letter-spacing: 3px;
    text-align: center;
    background-color: $primary-color1;
    border-radius: 0px;
    min-height: 70px;
    display: flex;
    align-items: center;
    justify-content: center;
    line-height: 24px;
    font-weight: 600;
    max-width: 340px;
    width: 100%;
    transition: 0.8s;
    outline: none;
    box-shadow: none;
    position: relative;

    &::before{
        content: ""; background-color: $sub-color2; width: 100%; height: 1px; position: absolute; top: -10px; transition: 0.8s;
    }

    &::after{
        content: ""; background-color: $sub-color2; width: 180px; height: 1px; position: absolute;   bottom: -10px;  left: 0; right: 0; margin: 0 auto; transition: 0.8s; display: flex;
    }

    &:hover, &:focus{
        background-color: $primary-color2;outline: none;
        box-shadow: none;
    }
}

.lines-hr { display: block; height: 8px; background-color: $black-color; width: 100%;
    &.lines-hr32{ width: 32px; }
    &.lines-hr2{ width: 220px; background-color: $primary-color1; }
}

/* add svg icons */

.mouse-icon{  -webkit-mask: url(../images/icons/mouse.svg) no-repeat 50% 50%; mask: url(../images/icons/mouse.svg) no-repeat 50% 50%; -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain; width: 35px; height: 57px; background-color: $black-color; }
.arrow-right-circle{  -webkit-mask: url(../images/icons/arrow-right-circle.svg) no-repeat 50% 50%; mask: url(../images/icons/arrow-right-circle.svg) no-repeat 50% 50%; -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain; width: 35px; height: 40px; background-color: $black-color; border-radius: 100%; }
.arrow-up-circle{ background-color: $sub-color1-50; height: 40px; width: 40px; transform: rotate(270deg); }
.arrow-right1{  -webkit-mask: url(../images/icons/arrow-right1.svg) no-repeat 50% 50%; mask: url(../images/icons/arrow-right1.svg) no-repeat 50% 50%; -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain; width: 31px; height: 18px; background-color: $black-color; border-radius: 100%;  }

.service-circle1{ -webkit-mask: url(../images/icons/service1.svg) no-repeat 50% 50%; mask: url(../images/icons/service1.svg) no-repeat 50% 50%; -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain; width: 22px; height: 17px; background-color: $white-color; }
.service-circle2{ -webkit-mask: url(../images/icons/service2.svg) no-repeat 50% 50%; mask: url(../images/icons/service2.svg) no-repeat 50% 50%; -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain; width: 20px; height: 20px; background-color: $white-color; }
.service-circle3{ -webkit-mask: url(../images/icons/service3.svg) no-repeat 50% 50%; mask: url(../images/icons/service3.svg) no-repeat 50% 50%; -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain; width: 22px; height: 20px; background-color: $white-color; }
.service-circle4{ -webkit-mask: url(../images/icons/service4.svg) no-repeat 50% 50%; mask: url(../images/icons/service4.svg) no-repeat 50% 50%; -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain; width: 20px; height: 18px; background-color: $white-color; }

.payment-span{  -webkit-mask-size: contain;  display: inline-block;  vertical-align: middle;  mask-size: contain;  background-color: #3F3F3F; }
.amazonpay-icon{ mask: url(../images/icons/payment/amazonpay.svg) no-repeat 50% 50%; width: 180px; height: 35px; }
.visa-icon{ mask: url(../images/icons/payment/visa.svg) no-repeat 50% 50%; width: 111px; height: 36px; }
.paypal-icon{ mask: url(../images/icons/payment/paypal.svg) no-repeat 50% 50%; width: 149px; height: 40px; }
.applepay-icon{ mask: url(../images/icons/payment/applepay.svg) no-repeat 50% 50%; width: 120px; height: 57px; }
.transferwise-icon{ mask: url(../images/icons/payment/transferwise.svg) no-repeat 50% 50%; width: 197px; height: 46px; }
.payoneer-icon{ mask: url(../images/icons/payment/payoneer.svg) no-repeat 50% 50%; width: 136px; height: 48px; }
.skrill-icon{ mask: url(../images/icons/payment/skrill.svg) no-repeat 50% 50%; width: 113px; height: 39px; }
.stripe-icon{ mask: url(../images/icons/payment/stripe.svg) no-repeat 50% 50%; width: 119px; height: 49px; }

/*  End of general css */

.line-1 {
    height: 1px; width: 100%; background-color: $sub-color2; position: absolute; top: 78px;
}

.column-40{ padding: 0; margin: 0; flex: 0 0 40%; max-width: 40%; position: relative; display: flex;  }
.column-45{ padding: 0; margin: 0; flex: 0 0 45%; max-width: 45%; position: relative; display: flex;  }
.column-50{ padding: 0; margin: 0; flex: 0 0 50%; max-width: 50%; position: relative;   }
.column-55{ padding: 0; margin: 0; flex: 0 0 55%; max-width: 55%; position: relative; display: flex;  }
.column-60{ padding: 0; margin: 0; flex: 0 0 60%; max-width: 60%; position: relative; display: flex;  }
.column-100{ padding: 0; margin: 0; flex: 0 0 100%; max-width: 100%; position: relative; display: flex;  }

.color-project1{ color: #930D17; }

.menubar-header{  
    padding: 0; margin: 0; position: fixed; top: 0; left: 0; width: 78px; position: fixed; min-height: 100vh; background-color: $sub-color1-50; z-index: 999;
    .header-div{
        padding: 0; margin: 0; position: relative; 

        .logo-div{ padding: 0; margin: 0; 
            .logo_link{ padding: 0; margin: 0; background-color: $primary-color1; display: flex; justify-content: center; align-items: center; min-height: 78px; width: 100%; }
        }

        .nav-center-div{ padding: 0; margin: 0; height: calc( 100vh - 78px ); display: flex; align-items: center; justify-content: center;
            .nav-m-bar{ padding: 0; margin: 0; 
                .menu-icon{
                    padding: 0; margin: 0; position: relative; height: 16px; width: 38px; 
                    .bar{ position: absolute; left: 0;  width: 36px; 
                        .fill{ width: 100%; height: 1px; background-color: $black-color; transition: background-color .2s ease-in-out; will-change: transform; }
                    }
                    .bar.top{ top: 0; }
                    .bar.middle{ top: 50%; margin-top: -.5px; }
                    .bar.bottom{ bottom: 0; }
                }
            }
            .open-nav {
                display: flex; align-items: center; justify-content: center;
            }
        }

    }
}

.banner-section{
    padding: 0; margin: 0; position: relative;
    .banner-div{
        margin: 0 0 0 0px; min-height: 100vh; display: flex; width: 100%; position: relative;
        .content-div {
            min-height: 100vh; padding: 30px 60px; padding-top: 77px;     display: flex; align-items: flex-start; flex-direction: column; width: 100%; justify-content: center; z-index: 99;     position: relative;
            h1 { padding: 0; margin: 0; font-size: 3.625rem; font-weight: 300; color: $black-color; line-height: 86px; }
            .bold-line{ font-weight: 600; position: relative; }
            a{ font-weight: 600; position: relative; color: $black-color; padding: 0 5px 0 0; text-decoration: underline; text-decoration-color: $primary-color1; }
            .heading-content{ padding: 0; margin: 0 0 30px 0; }
            .mouse-icon-div{
                padding: 0; margin: 40px 0 0px 0; .link{ display: flex; }
            }

            position: relative;
            .scroll-to-work{
                position: absolute;
                left: 60px;
                bottom: 50px;
                height: 120px;
                margin-bottom: 50px;
                a {
                    text-decoration: none;
                    span {
                        color: #FAFAFA;
                        font-weight: 400;
                        font-size: 18px;
                        display: block;
                        
                        img {
                            padding-left: 20px;
                            height: 100px;
                        }
                    }
                }
                
            }
        }
    }
    img.banner-img {
        width: 100%; height: 100vh; object-fit: cover; position: absolute; top: 0; left: 0; opacity: 0;
    }
}

.new-project{  
    padding: 0; margin: 0; position: fixed; top: 0; right: 0; width: 60px; position: fixed; min-height: 100vh; background-color: $sub-color1-50; z-index: 999;
    .new-project-top-div{
        padding: 0; margin: 0; 
        .project-btn-link{ padding: 0; margin: 0; background-color: $transparent-color; border:none; display: flex; justify-content: center; align-items: center; min-height: 78px; width: 100%; }
    }
    .new-project-link-middle{
        padding: 0; margin: 0; height: calc( 100vh - 78px );     display: flex; align-items: center; justify-content: center;
        .transform-link {
            transform: rotate(90deg);
            min-width: auto;
            color: $sub-color3;
            font-size: 18px;
            font-weight: 300;
            display: flex; white-space: nowrap; width: 100%;     min-width: 345px; transition: 0.8s; letter-spacing: 8px;
            &:hover, &:focus{
                text-decoration: none; color: $sub-color2;
            }
        }
    }
}

.case-study-section{
    padding: 0; margin: 0; position: relative;
    .case-study-div{
        padding: 100px 60px; margin: 0; background-color: $primary-color2;  position: relative;
        .rect-div { position: absolute; left: -60px; width: 100%; height: 100%; background-color: $black-color; z-index: 0; top: 0; }
        .case-study-root{
            padding: 0px 0 0 0; margin: 80px 0 30px 0; position: relative; display: flex;  flex-direction: column;

            .content-text-div{ padding: 0 30px; max-width: 750px;
                h3{ padding: 0; margin: 0 0 40px 0; font-size: 3.625rem; font-weight: 600; color: white; line-height: 72px; }
                .span-color1 { color: $color1; }
                .span-color2 { color: $color2; }
                .span-color3 { color: $color3; }
                .span-color4 { color: $color4; }
                p { font-size: 16px; }
            }

            .case-study-banner{
                padding: 0 0 0 95px; margin: 80px auto 0 auto; width: 100%;  position: relative; display: flex; justify-content: center; align-items: center;
                .case-study-cover {
                    max-width: 1100px; width: 100%; height: 100%; max-height: 795px; object-fit: cover;
                }
                .scroll-btn-div{
                    padding: 0; margin: 0; height: 100%; width: 95px; display: flex; align-items: flex-end; justify-content: center; position: relative;
                    .scroll-btn {
                        padding: 0; margin: 0; display: flex; align-items: center; justify-content: center; flex-direction: row; transform: rotate(90deg); font-size: 24px;
                        color: $sub-color1-50; text-transform: uppercase; font-weight: 300; letter-spacing: 10px; text-decoration: none;     position: absolute; bottom: 95px;
                        .arrow-down-circle{
                            background-color: $sub-color1-50; height: 53px; width: 53px; margin: 0 0 0 30px;
                        }
                    }
                }
            }

        }

        .slider-tab-fixed{
            padding: 0; margin: 0; position: fixed; top: 400px; left: 140px; z-index: 333;
           .slider-list-tabs {
                ul{ display: flex; flex-direction: column; list-style: none; }
                .slider-link {
                    height: 40px; width: 40px; display: flex; align-items: center; justify-content: center; background-color: $black-color; border-radius: 100%; color: $white-color; margin: 0 0 10px 0; text-decoration: none; transition:all 0.8s; font-weight: 500; font-size: 12px; border: none;
                    &:hover, &:focus{
                        background-color: $white-color; color: $black-color; text-decoration: none;  
                    }
                }
                .slider-link.active{ background-color: $white-color; color: $black-color; }
                a.top { display: flex; align-items: center; justify-content: center; margin: 0 0 30px 0; }
            }
        }

        .case-study-root{
            &::before{
                content: ""; display: flex; height: calc( 100% - 170px ); width: 45%; background-color: $black-color; position: absolute; left: -60px;
            }
        }

        span.common-inherit { font-style: normal; color: $white-color; }

        .case-study-pattern2{
            .patter2-full{
                padding: 40px 0 0 240px;
            }
            &::before{
                content: ""; display: flex; height: 620px; width: calc(100% - 130px ); background-color: black; position: absolute; right: -60px; left: auto;
            }
        }

    }
}

.services-section{
    padding: 100px 0; margin: 0; position: relative;
    .services-div{
        padding: 0; 

        .heading-div {
            max-width: 1000px;
            h2 {  margin: 0 0 20px 0; font-size: 3.625rem; color: $black-color; }
        }

        .services-root{
            padding: 0; margin:60px 0 0 0; display: flex; flex-wrap: wrap;
            .services-box {
                padding: 50px 40px 40px 40px; margin: 0; flex: 0 0 50%; max-width: 50%; min-height: 250px; display: flex; border: 1px solid $transparent-color;
                .services-icons {
                    padding: 0 15px; min-width: 74px; background-color: $sub-color1; height: 74px; border-radius: 100%; display: flex;
                    background-image: linear-gradient(180deg, #ED5C37 0%, #FF7D54 100%); border-radius: 100px; display: flex; align-items: center; justify-content: center;
                }
                .services-content {
                    width: calc(100% - 74px); padding: 0 20px 0 40px; margin: 0;
                    h4 { padding: 0; margin:0px 0 10px 0; font-size: 21px; font-weight: 700; color: $primary-color2; }
                    p { font-size: 15px; }
                    .link-box-div{ padding: 0; margin: 10px 0 0 0; position: relative; }
                    a.btn.btn-more {
                        padding: 0; margin: 0; font-size: 12px; text-transform: uppercase; font-weight: 600; color: $primary-color2; letter-spacing: 0.5px; transition: 0.8s;
                        &:hover, &:focus{
                            color: $primary-color1;
                        }
                    }
                }
                &.services-box1{ border-right:1px solid #DBDEE5; border-bottom:1px solid #DBDEE5;  }
                &.services-box2{ border-bottom:1px solid #DBDEE5; }
                &.services-box3{ border-right:1px solid #DBDEE5; }
            }
        }

    }
}

.footer-root{
    padding: 100px 0 30px 0; margin: 0; position: relative; background-color: $sub-color4;
    .messages-footer-div{
        padding: 0; position: relative;
        .messages-text-heading{
            padding: 0; margin: 0 0 50px 0; text-align: center; 
            h3{
                margin: 0 0 10px 0; font-size: 3.625rem; color: $primary-color2;
            }
        }
        .button-row{
            padding: 0; margin: 0px 0 20px 0; text-align: center;  display: flex; justify-content: center;
        }
    }

    .info-link-footer-div{
