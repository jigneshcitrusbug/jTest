                text-decoration: none;
                &::after {
                    content: "";
                    position: absolute;
                    bottom: 8px;
                    left: 5px;
                    width: 250px;
                    height: 5px;
                    display: inline-block;
                    border-radius: 100px;
                    background-color: #ed5c37;
                }
                &::before {
                    content: "";
                    position: absolute;
                    bottom: 8px;
                    right: 5px;
                    width: 75px;
                    height: 5px;
                    display: inline-block;
                    border-radius: 100px;
                    background-color: #ed5c37;
                }
            }

            p {
                padding: 0;
                margin: 0;
                font-size: 24px;
                font-weight: 300;
                color: #727272;
            }
        }
    }

    .our-technology-listing{
        display: flex;
        flex-wrap: wrap;
        flex-flow: wrap;
        
        .technology-box{
            width: 25%;
            display: inline-flex;
            flex-wrap: wrap;
            align-items: center;
            justify-content: center;
            min-height: 300px;
            background-color: transparent;
            border: 1px solid #E4E4E4;
            margin-top: -1px;
            margin-left: -1px;
            a {
                height: 100%;
                width: 100%;
                display: flex;
                align-items: center;
                justify-content: center;
                font-size: 30px;
                line-height: 46px;
                color: #181818;
                font-weight: 600;
                text-decoration: none;
                position: relative;
                transition: all .3s ease-in-out;
                .hover-description{
                    background-color: #ffffff;
                    padding: 30px 33px;
                    width: 100%;
                    height: 100%;
                    top: 0;
                    left: 0;
                    position: absolute;
                    opacity: 0;
                    display: flex;
                    align-items: center;
                    justify-content: flex-start;
                    flex-direction: column;
                    text-align: center;
                    transition: all .3s ease-in-out;
                    h3 {
                        font-size: 30px;
                        color: #000000;
                        margin-bottom: 25px;
                        font-weight: 600;
                    }
                    p {
                        font-size: 12px;
                        line-height: 22px;
                        color: #727272;
                        margin-bottom: 20px;
                        font-weight: 400;
                    }
                    p {
                        
                        span {
                            font-size: 12px;
                            line-height: 22px;
                            color: #000000;
                            font-weight: 400;
                        }
                    }
                    .btn-learn-more{
                        display: block;
                        background-color: #ed5c37;
                        border-radius: 100px;
                        line-height: 22px;
                        padding: 9px 52px;
                        color: #ffffff;
                        font-weight: 600;
                        position: absolute;
                        bottom: -20px;
                        margin: 0 auto;
                        min-height: 40px;
                        &:hover {
                            background-color: #F26820;
                        }
                    }
                }
                &:hover {
                    .hover-description{
                        opacity: 1;
                        transform: scale(1.15);
                    }
                }
            }
        }
    }


    .bg-ourtechnology{
        background-size: cover;
        background-position: center;
        height: 100%;
        width: 100%;
        position: absolute;
        left: 0;
        top:0;
        opacity: 0;
        &::before {
            content: '';
            left: 0;
            top: 0;
            position: absolute;
            height: 100%;
            width: 100%;
            background-color: #000000;
            opacity: 0.6;
        }
    }

   
}
